{
  "name": "bread",
  "version": "0.0.1",
  "description": "Data Structures and Algorithms designed for Reason",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": [
      "refmterr dune build ./src/bread_gen/BreadGen.exe",
      "refmterr dune build ./tests/TestExe.exe"
    ],
    "install": [
      "esy-installer bread.install"
    ]
  },
  "scripts": {
    "format": "./scripts/format",
    "test": "esy #{self.target_dir}/default/tests/TestExe.exe --onlyPrintDetailsForFailedSuites",
    "gen": "esy #{self.target_dir}/default/src/bread_gen/BreadGen.exe #{self.root}/lib/bread/"
  },
  "dependencies": {
    "ocaml": "^4.08.0",
    "@opam/dune": "^1.11.4",
    "@esy-ocaml/reason": "< 4.0.0"
  },
  "devDependencies": {
    "ocaml": "<= 4.08.0",
    "@reason-native/rely": "^3.1.0",
    "@reason-native/fp": "*",
    "@reason-native/fs": "*",
    "refmterr": "*",
    "@opam/merlin": "*"
  },
  "resolutions": {
    "@reason-native/fp": "facebookexperimental/reason-native:fp.json#d08d115"
  }
}
